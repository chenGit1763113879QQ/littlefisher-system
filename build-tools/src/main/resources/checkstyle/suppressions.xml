<?xml version="1.0"?>

<!DOCTYPE suppressions PUBLIC
        "-//Puppy Crawl//DTD Suppressions 1.1//EN"
        "http://checkstyle.sourceforge.net/dtds/suppressions_1_1.dtd">

<suppressions>
  <!--对mac生成的.DS_Store做忽略-->
  <suppress checks=".*" files=".DS_Store"/>

  <!--对所有的转换器类做忽略，不需要检查javadoc-->
  <suppress checks="JavadocMethodCheck" files=".*Converter.java"/>

  <!--对Mybatis Generator生成的Example不做检查-->
  <suppress checks=".*" files="dal[\\/]example[\\/]"/>

  <!--对Mybatis Generator生成的Model不做检查-->
  <suppress checks=".*" files="[\\/]model[\\/]"/>

</suppressions>